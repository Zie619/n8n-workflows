{
  "id": "test-workflow",
  "name": "测试工作流",
  "description": "这是一个用于测试的简单工作流",
  "nodes": [
    {
      "name": "定时触发器",
      "type": "n8n-nodes-base.cron",
      "parameters": {
        "interval": {
          "days": 1
        }
      }
    },
    {
      "name": "发送邮件",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "test@example.com",
        "subject": "测试邮件",
        "text": "这是一封测试邮件"
      }
    }
  ],
  "connections": {
    "定时触发器": {
      "main": [
        [
          {
            "node": "发送邮件",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true
}